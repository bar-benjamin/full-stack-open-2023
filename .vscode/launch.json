{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug Vite React",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:5173",
        "webRoot": "${workspaceFolder}",
        "sourceMapPathOverrides": {
          "webpack:///*": "${webRoot}/*",
          "webpack:///src/*": "${webRoot}/src/*"
        }
      }
    ]
  }
  